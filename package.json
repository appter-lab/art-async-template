{
  "name": "art-async-template",
  "typings": "index.d.ts",
  "description": "Clone of art-template with async/await support in templates.",
  "homepage": "http://github.com/appter-lab/art-async-template/",
  "version": "4.13.2",
  "keywords": [
    "template"
  ],
  "author": "appter-lab <exzod@pm.me>",
  "repository": {
    "type": "git",
    "url": "https://github.com/appter-lab/art-async-template.git"
  },
  "scripts": {
    "build": "npm run build-lib && npm run build-web",
    "build-lib": "rm -rf lib && babel src --out-dir lib",
    "build-web": "export NODE_ENV=production && webpack",
    "dev": "babel src --watch --out-dir lib",
    "test": "export NODE_ENV=production && istanbul cover node_modules/mocha/bin/_mocha -- --ui exports --colors 'test/**/*.js'",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "main": "index.js",
  "files": [
    "lib/",
    "index.d.ts"
  ],
  "engines": {
    "node": ">= 1.0.0"
  },
  "dependencies": {
    "acorn": "^7.1.0",
    "escodegen": "^1.14.1",
    "estraverse": "^4.3.0",
    "html-minifier": "^4.0.0",
    "is-keyword-js": "^1.0.3",
    "js-tokens": "^5.0.0",
    "merge-source-map": "^1.1.0",
    "source-map": "^0.7.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^7.0.1",
    "node-noop": "^1.0.0",
    "prettier": "^1.19.1",
    "webpack": "^4.41.5"
  },
  "license": "MIT"
}
